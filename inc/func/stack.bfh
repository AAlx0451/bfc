/* 
 * Actually, stack is hard to implement
 * but it'll be a builtin soon
 * this header is for backuping values
 * and follows guide/f.bfh
 */

{LOAD std/std.bfh}

{D PUSH 
CYCLE
    DECR
    {x1000{NEXT}}
    INCR
    {x1000{PREV}}
CYCL

TONULL}


{D POP
TONULL
{x1000{NEXT}}

CYCLE
    DECR
    {x1000{PREV}}
    INCR
    {x1000{NEXT}}
CYCL

{x1000{PREV}}}

{x72{INCR}} PUSH {x69{INCR}} PRINT POP PRINT
