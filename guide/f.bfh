/* function-like interface for brainfuck */

{LOAD ../std/std.bfh}

{D PROLOG 
CYCLE
    DECR
    {x100{NEXT}}
    INCR
    {x100{PREV}}
CYCL {! PROLOG}
}

{D EPILOG 
CYCLE
    DECR
    {x100{PREV}}
    INCR
    {x100{NEXT}}
CYCL {! EPILOG}
}

{D EPINEXT 
TONULL
NEXT
TONULL
PREV
}

{COM ENDF like bx lr}
{D ENDF
{x101{PREV}} {! ENDF}
}
